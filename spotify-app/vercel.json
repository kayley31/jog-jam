{
  "version": 2,
  "builds": [
    {
      "src": "running-app/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "node-server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "node-server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "running-app/build/$1"
    }
  ]
}
